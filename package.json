{
  "name": "geopkg",
  "version": "2.2.3",
  "description": "Tag npm modules with lat/long of where on the planet the module was published.",
  "bin": {
    "geopkg": "index.js"
  },
  "scripts": {
    "test": "standard && tape test.js",
    "postversion": "tag=`git describe --exact-match --tags $(git log -n1 --pretty='%h')` && node . && git add -A && git commit -C HEAD --amend && git tag $tag -f"
  },
  "keywords": [
    "geo",
    "geotag",
    "geotagging",
    "location",
    "coordinates",
    "npm",
    "module",
    "package",
    "package.json",
    "publish",
    "meta",
    "lat",
    "lon",
    "latitude",
    "longitude",
    "coordinates"
  ],
  "author": "Thomas Watson Steen <w@tson.dk>",
  "license": "MIT",
  "preferGlobal": true,
  "dependencies": {
    "opn": "^3.0.2",
    "place-geo-marker": "0.0.5",
    "wifi-triangulate": "^1.1.1"
  },
  "devDependencies": {
    "standard": "^4.3.1",
    "tape": "^4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/watson/geopkg.git"
  },
  "bugs": {
    "url": "https://github.com/watson/geopkg/issues"
  },
  "homepage": "https://github.com/watson/geopkg",
  "coordinates": [
    53.03143521103752,
    13.30494579928586
  ]
}
